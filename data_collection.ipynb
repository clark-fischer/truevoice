    "\n",
    "def calculate_vote_seat_share(data,json, planType ):\n",
    "\n",
    "    districts = data[\"districts\"]\n",
    "    curve_data = []\n",
    "\n",
    "    for district in districts:\n",
    "        dem_vote_share = district[\"democratsPercentage\"]\n",
    "        rep_vote_share = 1 - district[\"democratsPercentage\"]\n",
    "        \n",
    "        # Seat share is 1 if the party has a majority (> 50%), else 0\n",
    "        dem_seat_share = 1 if dem_vote_share > 0.5 else 0\n",
    "        rep_seat_share = 1 if rep_vote_share > 0.5 else 0\n",
    "\n",
    "        curve_data.append({\n",
    "            \"districtId\": district[\"districtId\"],\n",
    "            \"demVoteShare\": dem_vote_share,\n",
    "            \"demSeatShare\": dem_seat_share,\n",
    "            \"repVoteShare\": rep_vote_share,\n",
    "            \"repSeatShare\": rep_seat_share\n",
    "        })\n",
    "\n",
    "    json[\"curveData\"] = curve_data\n",
    "    json[\"totalDistricts\"] = len(districts)\n",
    "    json[\"planType\"]= planType\n",
    "\n",
    "calculate_vote_seat_share(nv_repFav, nv_voteShare, \"repFavored\")\n",
    "nv_voteShare"
